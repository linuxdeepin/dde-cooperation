// version.rc.in
${SET_ICON}
${SET_ADMIN}

1 VERSIONINFO
FILEVERSION     APP_VERSION_MAJOR, APP_VERSION_MINOR, APP_VERSION_PATCH, APP_BUILD_NUMBER
PRODUCTVERSION  APP_VERSION_MAJOR, APP_VERSION_MINOR, APP_VERSION_PATCH, APP_BUILD_NUMBER
FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
FILEOS 0x40004L
FILETYPE 0x1L
FILESUBTYPE 0x0L
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904b0"
    BEGIN
      VALUE "CompanyName", "Uniontech, Inc."
      VALUE "CompanyWeb", "https://www.deepin.org/"
      VALUE "FileDescription", "${PRODUCT_INFO}"
      VALUE "FileVersion", "${APP_VERSION}"
      VALUE "LegalCopyright", "Copyright (C) 2023-2024 Uniontech Inc."
      VALUE "OriginalFilename", "${PROJ_NAME}.exe"
      VALUE "ProductName", "${PRODUCT_NAME}"
      VALUE "ProductVersion", "${APP_VERSION}"
      VALUE "InternalName", "${PROJ_NAME}"
    END
  END

  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x409, 1200
  END
END
